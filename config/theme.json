      "box_shadow": {
        "inner": "inset 0 1px 1px rgba(255, 255, 255, 0.1), inset 0 -1px 1px rgba(0, 0, 0, 0.5)",
        "outer": "0 8px 32px rgba(0, 255, 255, 0.1), 0 0 20px rgba(0, 255, 255, 0.05)",
        "neon": "0 0 20px rgba(0, 255, 255, 0.3), 0 0 40px rgba(255, 0, 255, 0.1)"
      },
      "description": "*Reality filtered through neon-soaked glass*"
    },
