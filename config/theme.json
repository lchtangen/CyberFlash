  "animations_cyberpunk": {
    "timing": {
      "instant": "50ms",
      "fast": "150ms",
      "normal": "300ms",
      "slow": "500ms",
      "epic": "1000ms"
    },
    "easing": {
      "linear": "linear",
      "ease_out": "cubic-bezier(0.4, 0, 0.2, 1)",
      "ease_in": "cubic-bezier(0.4, 0, 1, 1)",
      "ease_in_out": "cubic-bezier(0.4, 0, 0.2, 1)",
      "spring": "cubic-bezier(0.34, 1.56, 0.64, 1)",
      "cyber_pulse": "cubic-bezier(0.36, 0, 0.66, 1)",
      "neon_bounce": "cubic-bezier(0.68, -0.55, 0.265, 1.55)"
    },
    "entrance": {
      "fade_in": {
        "duration": "300ms",
        "easing": "ease_out",
        "description": "*Digital consciousness awakening*"
      },
      "glow_in": {
        "duration": "400ms",
        "easing": "ease_out",
        "keyframes": {"from": "opacity: 0; filter: drop-shadow(0 0 0px #00FFFF)", "to": "opacity: 1; filter: drop-shadow(0 0 15px #00FFFF)"},
        "description": "*Neon birth in the digital realm*"
      },
      "slide_up_neon": {
        "duration": "400ms",
        "easing": "spring",
        "keyframes": {"from": "transform: translateY(30px); opacity: 0; filter: drop-shadow(0 0 0px #00FFFF)", "to": "transform: translateY(0); opacity: 1; filter: drop-shadow(0 0 10px #00FFFF)"},
        "description": "*Rising from the digital abyss*"
      },
      "scale_neon": {
        "duration": "400ms",
        "easing": "neon_bounce",
        "keyframes": {"from": "transform: scale(0.8); opacity: 0; filter: drop-shadow(0 0 0px #FF00FF)", "to": "transform: scale(1); opacity: 1; filter: drop-shadow(0 0 15px #FF00FF)"},
        "description": "*Reality expansion in neon light*"
      }
    },
    "interaction": {
      "button_glow": {
        "duration": "150ms",
        "easing": "ease_out",
        "keyframes": {"from": "filter: drop-shadow(0 0 5px currentColor)", "to": "filter: drop-shadow(0 0 15px currentColor)"},
        "description": "*Neon intensification at touch*"
      },
      "hover_elevation": {
        "duration": "200ms",
        "easing": "ease_out",
        "keyframes": {"from": "transform: translateY(0); box-shadow: 0 8px 24px rgba(0, 255, 255, 0.12)", "to": "transform: translateY(-4px); box-shadow: 0 0 30px rgba(0, 255, 255, 0.3)"},
        "description": "*Glass rising into immersion*"
      },
      "pulse_neon": {
        "duration": "2000ms",
        "easing": "ease_in_out",
        "repeat": "infinite",
        "keyframes": {"0%": "filter: drop-shadow(0 0 5px #00FFFF)", "50%": "filter: drop-shadow(0 0 20px #00FFFF)", "100%": "filter: drop-shadow(0 0 5px #00FFFF)"},
        "description": "*Breathing heartbeat of energy*"
      },
      "glitch_effect": {
        "duration": "100ms",
        "repeat": 3,
        "easing": "linear",
        "keyframes": {"0%": "transform: translateX(0)", "25%": "transform: translateX(-2px)", "50%": "transform: translateX(2px)", "75%": "transform: translateX(-1px)", "100%": "transform: translateX(0)"},
        "description": "*Cybernetic distortion of reality*"
      },
      "scan_line": {
        "duration": "3000ms",
        "easing": "linear",
        "repeat": "infinite",
        "keyframes": {"0%": "transform: translateY(-100%)", "100%": "transform: translateY(100%)"},
        "description": "*CRT scan reality overlay*"
      }
    },
    "state_transitions": {
      "loading_spin": {
        "duration": "1000ms",
        "easing": "linear",
        "repeat": "infinite",
        "description": "*Awaiting digital transcendence*"
      },
      "success_burst": {
        "duration": "600ms",
        "easing": "ease_out",
        "effects": ["glow", "pulse", "confetti_neon"],
        "description": "*Victory in the digital realm*"
      },
      "error_vibrate": {
        "duration": "150ms",
        "repeat": 3,
        "easing": "ease_in_out",
        "description": "*System warning in neon red*"
      }
    }
  },
  "component_styles_cyberpunk": {
    "button": {
      "base": {
        "padding": "10px 20px",
        "border_radius": "6px",
        "border": "2px solid transparent",
        "font_weight": "600",
        "font_size": "14px",
        "font_family": "JetBrains Mono",
        "transition": "all 150ms cubic-bezier(0.34, 1.56, 0.64, 1)",
        "cursor": "pointer",
        "text_transform": "uppercase",
        "letter_spacing": "1px"
      },
      "primary": {
        "background": "linear-gradient(135deg, #00FFFF 0%, #00CCFF 100%)",
        "color": "#0A0E27",
        "border": "2px solid #00FFFF",
        "box_shadow": "0 0 20px rgba(0, 255, 255, 0.4), inset 0 1px 1px rgba(255, 255, 255, 0.3)",
