  "glass_effect_cyberpunk": {
    "material": {
      "name": "üîÆ Neon Glass",
      "background": "rgba(26, 31, 58, 0.6)",
      "backdrop_filter": "blur(20px) saturate(180%)",
      "border": {
        "color": "rgba(0, 255, 255, 0.3)",
